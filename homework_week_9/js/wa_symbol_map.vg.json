{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 800,
    "height": 450,
    "title": "Primary School Rank in Western Australia",
    "projection": {"type": "equirectangular"},
    "layer": [
      {
        "data": {
          "url": "https://raw.githubusercontent.com/mlay0009/fit3179/homework_week_9/js/MB_2016_WA.json",
          "format": {"type": "topojson", "feature": "MB_2016_WA"}
        },
        "mark": {"type": "geoshape", "fill": "lightgray", "stroke": "black"}
      },
      {
        "data": {
          "url": "https://raw.githubusercontent.com/mlay0009/fit3179/homework_week_9/js/ne_110m.topojson",
          "format": {"type": "topojson", "feature": "ne_110m_graticules_30"}
        },
        "mark": {"type": "geoshape", "fill": null, "stroke": "lightgray"}
      },
      {
        "data": {
          "url": "https://raw.githubusercontent.com/mlay0009/fit3179/homework_week_9/data/wa_primary_schools.csv"
        },
        "mark": {"type": "circle", "tooltip": {"content": "data"}},
        "encoding": {
          "longitude": {"field": "longitude", "type": "quantitative"},
          "latitude": {"field": "latitude", "type": "quantitative"},
          "size": {
            "field": "Total Students",
            "type": "quantitative",
            "title": "Total Students (Richter scale)",
            "scale": {"domain": [0,5000]}
          }
        }
      }
    ]
  }