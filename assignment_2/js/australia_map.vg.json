{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": "2022 Australia Student Enrolments by Region",
    "width": 800,
    "height": 400,
    "projection": {"type": "equalEarth"},
    "data": {
      "url": "https://raw.githubusercontent.com/mlay0009/fit3179/main/assignment_2/js/australia.topojson",
      "format": {"type": "topojson", "feature": "ne_10m_admin_1_states_provinces"}
    },
    "transform": [
      {
        "lookup": "properties.NAME",
        "from": {
          "data": {
            "url": "https://raw.githubusercontent.com/mlay0009/fit3179/main/assignment_2/data/aus_enrolments.csv"
          },
          "key": "State",
          "fields": ["Government"]
        }
      }
    ],
    "mark": {"type": "geoshape"},
    "encoding": {
      "color": {
        "field": "Government",
        "type": "quantitative"
      },
      "tooltip": [
        {"field": "State", "type": "nominal", "title": "State"},
        {"field": "Government", "type": "quantitative"}
      ]
    }
  }