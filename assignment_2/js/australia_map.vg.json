{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": "2022 Australia Student Enrolments by Region",
    "width": 800,
    "height": 400,
    "projection": {"type": "equirectangular"},
    "data": {
      "url": "https://raw.githubusercontent.com/mlay0009/fit3179/main/assignment_2/data/aus_enrolments.csv"
    },
    "layer" : [
      {
        "data": {
          "url": "https://raw.githubusercontent.com/mlay0009/fit3179/main/assignment_2/js/australia.json",
          "format": { "type": "topojson", "feature": "ne_50m_admin_1_states_provinces" }
        },
        "transform": [
          {
            "calculate": "'Data is not available in ' + datum.properties.NAME",
            "as": "note"
          }
        ],
        "mark": {"type": "geoshape", "fill": "lightgray", "stroke": "white"},
        "encoding": {"tooltip": {"field": "note"}}
      },
      {
        "data" : {"url": "https://raw.githubusercontent.com/mlay0009/fit3179/main/assignment_2/data/aus_enrolments.csv"},
        "transform": [
          {
            "lookup" : "State", 
            "from" : { 
              "data" : {"url" : "https://raw.githubusercontent.com/mlay0009/fit3179/main/assignment_2/js/australia.json",
              "format": {"type": "topojson", "feature": "ne_50m_admin_1_states_provinces"}
            },
          "key": "properties.NAME"
          },
          "as": "geo"
        }],
      "mark": {"type": "geoshape", "stroke": "white"},
      "encoding": {
        "shape": {"field": "geo", "type": "geojson"},
        "color": {
          "field": "Government",
          "type": "quantitative"
        },
        "tooltip": [
          {"field": "State", "type": "nominal"},
          {"field": "Government", "type": "quantitative"}
        ]
      }
    }
  ]
}