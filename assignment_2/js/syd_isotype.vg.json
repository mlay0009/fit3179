{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "height": 300,
    "width": 300,
    "data": {
        "url": "https://raw.githubusercontent.com/mlay0009/fit3179/main/assignment_2/data/sydney_isotype.csv"
    },
    "mark": {
        "type": "circle",
        "filled": true,
        "size": 400
    },
    "encoding": {
        "x": {
            "field": "Column",
            "type": "quantitative",
            "axis": null
        },
        "y": {
            "field": "Row",
            "type": "quantitative",
            "axis": null,
            "sort": "y"
        },
        "color": {
            "field": "Type",
            "type": "nominal",
            "scale": {
                "domain": [
                    "Primary",
                    "Combined"
                ],
                "range": [
                    "#994d00",
                    "#cccccc"
                ]
            },
            "legend": null
        }
    },
    "config": {
        "background": "transparent",
        "view": {
            "stroke": null
        }
    }
}