{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
        "url": "https://raw.githubusercontent.com/mlay0009/fit3179/main/assignment_2/data/sydney_donut.csv"
    },
    "mark": "arc",
    "encoding": {
      "theta": {"field": "Count of Year_Range", "type": "quantitative", "stack": true, "title": "Count of Year Range"},
      "color": {"field": "Year_Range", "type": "nominal", "legend": {"title": "Year Range"}},
      "tooltip": [
        {"field": "Year_Range", "type": "nominal", "title": "Year Range"},
        {"field": "Count of Year_Range", "type": "quantitative", "title": "Total Schools"}
      ]
    },
    "view": {"stroke": null},
    "width": 200,
    "height": 200,
    "config": {
      "arc": {"innerRadius": 60}
    }
  }
  