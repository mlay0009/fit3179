{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 800,
  "height": 450,
  "title": "Primary School Rank in New South Wales",
  "data": {
    "url": "https://raw.githubusercontent.com/mlay0009/fit3179/main/homework_week_10/data/clean.csv"
  },
  "projection": {"type": "equirectangular"},
  "layer": [
    {
      "data": {
        "url": "https://raw.githubusercontent.com/mlay0009/fit3179/main/homework_week_10/js/sydney_localities.json",
        "format": {"type": "topojson", "feature": "nsw_localities"}
      },
      "mark": {"type": "geoshape", "fill": "lightgray", "stroke": "black"}
    },
    {
      "data": {
        "url": "https://raw.githubusercontent.com/mlay0009/fit3179/main/homework_week_10/data/clean.csv"
      },
      "transform": [
        {
          "lookup": "Suburb",
          "from": {
            "data": {
              "url": "https://raw.githubusercontent.com/mlay0009/fit3179/main/homework_week_10/js/sydney_localities.json",
              "format": {"type": "topojson", "feature": "nsw_localities"}
            },
            "key": "properties.NAME"
          },
          "as" : "geo"
        }
      ],
      "mark": {
        "type": "geoshape", "stroke" : "white"
      },
      "encoding": {
        "shape": {"field": "geo", "type": "geojson"},
        "color": {"field": "Acceptance Rate", "type": "quantitative", "scale": {"scheme": "blues"}},
        "tooltip": [
          {"field": "Suburb", "title": "Suburb"}
        ]
      }
    }
  ]
}